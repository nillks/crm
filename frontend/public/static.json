{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "headers": {
    "/**": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/*.{js,css,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot}": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}

